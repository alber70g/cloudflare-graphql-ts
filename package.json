{
  "name": "cloudflare-ts-worker",
  "version": "1.0.0",
  "description": "Starter kit template for using Typescript with a Cloudflare Worker",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "lint-fix": "tslint --project tsconfig.json --fix",
    "test": "jest",
    "build": "webpack --progress --mode production",
    "build-dev": "webpack --progress --mode none",
    "build-all": "npm run lint && npm run test && npm run build",
    "publish": "wrangler publish",
    "build:publish": "npm run build-dev && npm run publish"
  },
  "author": "alber70g",
  "license": "MIT",
  "dependencies": {
    "graphql": "^14.4.2"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.4",
    "@udacity/types-service-worker-mock": "^1.0.1",
    "cloudflare-worker-mock": "^1.0.1",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "types-cloudflare-worker": "^1.0.1",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6"
  }
}
